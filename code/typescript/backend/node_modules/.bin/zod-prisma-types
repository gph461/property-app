#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/zod-prisma-types@2.7.9/node_modules/zod-prisma-types/dist/node_modules:/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/zod-prisma-types@2.7.9/node_modules/zod-prisma-types/node_modules:/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/zod-prisma-types@2.7.9/node_modules:/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/zod-prisma-types@2.7.9/node_modules/zod-prisma-types/dist/node_modules:/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/zod-prisma-types@2.7.9/node_modules/zod-prisma-types/node_modules:/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/zod-prisma-types@2.7.9/node_modules:/mnt/c/Netseal/sg_group/code/typescript/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../zod-prisma-types/dist/bin.js" "$@"
else
  exec node  "$basedir/../zod-prisma-types/dist/bin.js" "$@"
fi
